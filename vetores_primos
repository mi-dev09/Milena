#include <stdio.h>

int ehPrimo(int n) {
    if (n < 2) return 0;
    for (int i = 2; i * i <= n; i++) {
        if (n % i == 0) return 0;
    }
    return 1;
}

int main() {
    int n;

    printf("Informe o tamanho do array: ");
    scanf("%d", &n);

    int vet[n];

    for (int i = 0; i < n; i++) {
        int valor;
        do {
            printf("Digite um valor positivo e impar para vet[%d]: ", i);
            scanf("%d", &valor);
        } while (valor <= 0 || valor % 2 == 0); 
        vet[i] = valor;
    }

    printf("\nNumeros primos do array:\n");
    for (int i = 0; i < n; i++) {
        if (ehPrimo(vet[i])) {
            printf("%d ", vet[i]);
        }
    }

    printf("\n");
    return 0;
}
